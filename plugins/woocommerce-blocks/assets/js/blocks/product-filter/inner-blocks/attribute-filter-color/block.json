{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "woocommerce/product-filter-attribute-color",
	"version": "1.0.0",
	"title": "Product Filter: Attribute Color",
	"description": "A block to filter products by color attribute.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"textdomain": "woocommerce",
	"apiVersion": 2,
	"ancestor": [ "woocommerce/product-filter-attribute" ],
	"supports": {
		"interactivity": true,
		"inserter": false
	},
	"usesContext": [ "filterOptions", "attributeTerms" ],
	"attributes": {
		"termColors": {
			"type": "object",
			"default": {}
		},
		"displayStyle": {
			"type": "string",
			"default": "list"
		}
	}
}
